.rating p {
  display: flex;
}

.rating {
  display: flex;
  flex-direction: row-reverse;
  align-items: end;
}

.star-checkbox {
  display: inline-block;
  margin-right: 10px;
  cursor: pointer;
}

.star-checkbox br,
.contact-submit br {
  display: none;
}

.star {
  transition: color 0.3s ease;
  width: 30px;
  height: 30px;
  background: url('/wp-content/uploads/2023/08/star.png') no-repeat;
  background-size: cover;
  cursor: pointer;
  display: block;
}

.star.checked {
  background-image: url('/wp-content/uploads/2023/08/star-gold.png');
}

.star-checkbox input {
