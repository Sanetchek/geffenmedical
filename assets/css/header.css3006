@import url('https://fonts.googleapis.com/css2?family=Assistant:wght@200;300;400;500;600;700;800&family=Nunito:ital,wght@0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;0,1000;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900;1,1000&display=swap');

body {
  unicode-bidi: embed;
}

body * {
  font-family: 'Assistant', sans-serif;
}

p {
  margin-bottom: 0 !important;
  font-family: 'Assistant';
}

em {
  font-style: italic !important;
}

.ltr {
  direction: ltr !important;
}

* {
  text-decoration: none !important;
}

a,
abbr,
acronym,
address,
applet,
article,
aside,
audio,
b,
big,
blockquote,
body,
canvas,
caption,
center,
cite,
code,
dd,
del,
details,
dfn,
div,
dl,
dt,
em,
embed,
fieldset,
figcaption,
figure,
footer,
form,
h1,
h2,
h3,
h4,
h5,
h6,
header,
hgroup,
html,
i,
iframe,
img,
ins,
kbd,
label,
legend,
li,
mark,
menu,
nav,
object,
ol,
output,
p,
pre,
q,
ruby,
s,
samp,
section,
small,
span,
strike,
strong,
sub,
summary,
sup,
table,
tbody,
td,
tfoot,
th,
thead,
time,
tr,
tt,
u,
ul,
var,
video {
  margin: 0;
  padding: 0;
  border: 0;
  font-size: 100%;
  font: inherit;
  vertical-align: baseline;
  font-family: 'Assistant';
}

.columns-wrapper {
  display: flex;
}

* {
  font-family: "Assistant", sans-serif;
}

main {
  direction: rtl;
}

h1,
h2,
h3,
h4,
h5,
h6 {
  margin-top: 0;
  margin-bottom: 0.5rem !important;
}

p {
  margin-top: 0;
  margin-bottom: 1rem;
  font-family: 'Assistant';
}

/* Chrome, Safari, Edge, Opera */
input::-webkit-outer-spin-button,
input::-webkit-inner-spin-button {
  -webkit-appearance: none;
  margin: 0;
}

/* Firefox */
input[type=number] {
  -moz-appearance: textfield;
}

img {
  display: block;
  max-width: 100%;
  height: auto;
}

ul {
  list-style: none;
  margin: 0;
  padding: 0;
}

p {
  direction: rtl;
  margin-top: 0 !important;
  margin-bottom: 1rem !important;
}

a {
  text-decoration: none;
}

/*----------------------------------HEADER-----------------------------------------------*/
.site-branding>a {
  text-align: center;
  display: inline-block;
}

.site-title {
  display: none;
}

.menu-main-geffen-row {
  background: #FFF;
  box-shadow: 1px 14px 20px 0px #C4E7FB;
  text-align: center;
  display: flex;
  justify-content: center;
  padding: 0px 54px 0px 117px;
}

.menu-main-geffen-container {
  display: flex;
  justify-content: center;
  color: #0A3B64;
  text-align: right;
  font-family: Assistant;
  font-size: 16px;
  font-style: normal;
  font-weight: 600;
  line-height: normal;
  height: 50px;
  align-items: center;
}

.menu-main-geffen-container a {
  color: #0A3B64 !important;
  font-weight: 600;
  font-size: 18px;
}

.menu-item-has-children a,
.category-sub-menu a {
  position: relative;
  padding-left: 20px;
}

.cart__total_text {
  border-bottom: none !important;
}

.menu-item-has-children:before,
.category-sub-menu:before {
  content: '';
  position: absolute;
  width: 26px;
  height: 26px;
  background: url('../img/chevron-down.png') no-repeat;
  display: block;
  top: 9px;
  left: 0;
}

.menu-item-has-children:hover:before,
.category-sub-menu.active:before {
  content: '';
  transform: rotate(180deg);
  top: -12px;
  left: -16px;
}

.main-navigation-category {
  position: absolute;
  left: 0;
  right: 0;
  z-index: 1000;
  box-shadow: 0px 2px 21px 5px #C4E7FB4D;
  background: #fff;
  padding: 42px;
  display: none;
}

.main-navigation-category-wrap {
  max-width: 763px;
  margin: 0 auto;
  display: flex;
  flex-wrap: wrap;
  gap: 40px;
  background: #fff;
  justify-content: center;
  align-items: center;
  transition: all 0.5s ease;
}

.main-navigation-category-wrap a:hover .main-page-menu-item {
  background-color: #0A3B64;
}

.main-navigation-category-wrap a:hover .main-page-menu-item img {
  filter: brightness(0) invert(1);
}

header.site-header {
  display: flex;
  padding: 20px  117px 7px;
  align-items: center;
  justify-content: space-between;
  flex-direction: row-reverse;
}

.ds-header-menu2 {
  position: fixed;
  width: 100%;
  height: 90px;
  z-index: 998;
  top: 80px;
  display: flex;
  flex-direction: row-reverse;
  align-items: center;
  background: #fff;
  box-shadow: 0px 4px 24.3px 0px rgba(0, 0, 0, 0.25);
}

.admin-bar .ds-header-menu2 {
  top: 112px;
}

header.site-header.des-header {
  position: fixed !important;
  width: 100%;
  height: 90px;
  top: 0;
  z-index: 999;
  background: #fff;
}

.admin-bar header.site-header.des-header {
  top: 32px;
}

.multiple-items {
  display: flex;
  overflow-x: hidden;
}

#profile_menu_popup {
  display: none;
  position: absolute;
  top: 39px;
  left: -52px;
  z-index: 100;
  border-radius: 30px;
  background: #C4E7FB;
  width: 150px;
}

.profile-nav {
  display: flex;
  flex-direction: column;
}

#image-65f68960c3>img {
  max-height: 377px;
  height: 100%;
}

#profile_menu_popup a {
  padding: 20px;
  color: #0A3B64 !important;
  display: block;
  text-align: center;
  width: 100%;
}

#profile_menu_popup>svg {
  position: absolute;
  top: -13%;
  left: 50%;
  transform: translate(-50%, 0px);
}

#confirm_logout_popup {
  display: none;
  position: fixed;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  z-index: 100;
}

.confirm-logout {
  position: relative;
  display: flex;
  align-items: center;
}

.confirm-logout-title {
  color: #0A3B64;
  text-align: center;
  font-size: 27px;
  font-style: normal;
  font-weight: 700;
  line-height: 135.3%;
  margin-bottom: 36px !important;
}

.confirm-logout-wrap {
  position: relative;
  background: #fff;
  padding: 40px 80px 40px 80px !important;
  border-radius: 114px 0 0 114px;
  box-shadow: 0px 0px 0px 1px rgba(0, 0, 0, 0.06), 0px 10px 36px 0px rgba(0, 0, 0, 0.16);
}

.confirm-logout-btns {
  display: flex;
  align-items: center;
  justify-content: center;
  gap: 16px;
}

#close_confirm_logout {
  position: absolute;
  top: 30px;
  left: 185px;
  z-index: 10;
  font-size: 42px;
  line-height: 0;
  color: #565D65;
  cursor: pointer;
}

.btn-blue {
  display: inline-flex !important;
  padding: 11px 35px;
  justify-content: center;
  align-items: center;
  gap: 10px;
  border-radius: 8px;
  background: #0A3B64;
  color: #FFF !important;
  font-size: 18px;
  font-weight: 700;
  line-height: normal;
  border: 1px solid #0A3B64;
}

.white-btn {
  display: inline-flex;
  padding: 11px 35px;
  justify-content: center;
  align-items: center;
  border-radius: 8px;
  background: #FFF;
  color: #0A3B64 !important;
  font-size: 18px;
  font-weight: 700;
  line-height: normal;
  border: 1px solid #0A3B64;
}

/* Navigation
--------------------------------------------- */
.main-navigation {
  display: block;
  width: 100%;
}

.main-navigation ul {
  display: none;
  list-style: none;
  margin: 0;
  padding-left: 0;
}

.main-navigation ul ul {
  box-shadow: 0 3px 3px rgba(0, 0, 0, 0.2);
  position: absolute;
  top: 100%;
  z-index: 99999;
  display: none;
  background: #fff;
  flex-direction: column;
}

.main-navigation li:hover ul {
  display: flex;
}

.main-navigation ul ul ul {
  top: 0;
}

.main-navigation ul ul li:hover>ul,
.main-navigation ul ul li.focus>ul {
  display: block;
  left: auto;
}

.main-navigation ul ul a {
  width: 200px;
  padding: 12px 20px;
}

.main-navigation ul ul a:hover {
  background-color: #337ab7;
  color: #fff !important;
}

.main-navigation ul li:hover>ul,
.main-navigation ul li.focus>ul {
  left: auto;
}

.main-navigation li {
  position: relative;
}

.main-navigation a {
  display: block;
  text-decoration: none;
}

/* Small menu. */
.menu-toggle,
.main-navigation.toggled ul {
  display: block;
}

@media screen and (min-width: 37.5em) {
  .menu-toggle {
    display: none;
  }

  .main-navigation ul.menu {
    display: flex;
    flex-direction: row-reverse;
    gap: 60px;
    justify-content: center;
    align-items: center;
  }
}

.site-main .comment-navigation,
.site-main .posts-navigation,
.site-main .post-navigation {
  margin: 0 0 1.5em;
}

.comment-navigation .nav-links,
.posts-navigation .nav-links,
.post-navigation .nav-links {
  display: flex;
}

.comment-navigation .nav-previous,
.posts-navigation .nav-previous,
.post-navigation .nav-previous {
  flex: 1 0 50%;
}

.comment-navigation .nav-next,
.posts-navigation .nav-next,
.post-navigation .nav-next {
  text-align: end;
  flex: 1 0 50%;
}

.user-site-navigation {
  display: flex;
  flex-direction: row-reverse;
}

.user-site-navigation div {
  padding: 0 10px;
}

.user-site-search,
.search__label {
  position: relative;
}

.search__popup {
  display: none;
  position: absolute;
  top: -5px;
  left: -7px;
  z-index: 100;
  padding: 0 !important;
}

.search__form {
  display: flex;
  align-items: center;
  gap: 8px;
  border-radius: 34px;
  border: 0.7px solid rgba(10, 59, 100, 0.30);
  background: #FFF;
  padding: 7px 16px;
}

.search__input {
  border: 0;
  background: #fff;
}

.search__input::placeholder {
  color: #0A3B64;
  font-size: 16px;
  font-weight: 400;
  line-height: normal;
}

.search__btn {
  background: transparent;
  border: 0;
  padding: 0;
  line-height: 0;
}

.search__close {
  display: block;
  font-size: 32px;
  font-weight: 500;
  color: #0a3b64;
  line-height: 0;
  cursor: pointer;
}

.navbar-second-site-navigation {
  padding: 15px;
  display: flex;
  justify-content: start;
  align-items: center;
}

.site-header .site-branding {
  text-align: center;
}

.hamburger-second-site-navigation {
  cursor: pointer;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  width: 25px;
  height: 20px;
}

.line-second-site-navigation {
  width: 100%;
  height: 2px;
  background-color: #0A3B64;
  transition: transform 0.3s;
}

.line-second-site-navigation.line1 {
  transform-origin: top;
}

.line-second-site-navigation.line2 {
  transform-origin: center;
}

.menu-second-site-navigation {
  display: none;
  flex-direction: column;
  align-items: center;
  position: absolute;
  top: 120px;
  right: 65px;
  background: linear-gradient(0deg, rgba(231, 240, 247, 0.70) 0%, rgba(231, 240, 247, 0.70) 100%), #FFF;
  transition: transform 0.3s;
  transform: translateY(-100%);
  z-index: 2;
  border-radius: 10px;
}

.menu-second-site-navigation li a {
  color: #0A3B64 !important;
  text-align: right;
  font-family: Assistant;
  font-size: 16px;
  font-style: normal;
  font-weight: 600;
  line-height: normal;
  width: 226px;
  height: 59px;
  display: block;
  padding: 20px 29px 21px 0;
}

.menu-second-site-navigation li a:hover {
  color: #fff !important;
  background: #0A3B64;
}

.menu-second-site-navigation.show {
  display: flex;
  transform: translateY(0);
}


.hamburger-second-site-navigation .line1 {
  transform: translateX(0);
}

.hamburger-second-site-navigation .line2 {
  transform: translateX(0);
}


.hamburger-second-site-navigation.open .line1 {
  width: 50%;
  transform-origin: right;
}

.hamburger-second-site-navigation.open .line2 {
  width: 70%;
  transform-origin: right;

}

/*end-navbar-second-site-navigation*/

.user-site-login {
  border-right: 2px solid #0A3B64;
  position: relative;
  cursor: pointer;
}

/*--------------------------------END_HEADER------------------------------------------*/


